{
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "plugins": ["react"],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "react/prop-types": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}
