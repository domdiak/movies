{
    "extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
    "plugins": ["plugin:react/recommended", "jest"],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    }
}
