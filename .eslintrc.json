{
    "extends": [
        "eslint:recommended",
        "eslint-plugin-jest",
        "eslint-plugin-react",
        "prettier"
    ],
    "plugins": ["react"],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}
